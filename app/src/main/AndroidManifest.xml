<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luis.agenda">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/calender_256"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Contacto"
            android:label="Contactos Asociados" />
        <activity
            android:name=".AgregarCliente"
            android:label="Agregar Cliente" />
        <activity android:name=".AgregarContacto"
            android:label="Agregar Contacto"/>
        <activity android:name=".DetalleContacto"
            android:label="Detalles del Contacto" ></activity>
    </application>

</manifest>